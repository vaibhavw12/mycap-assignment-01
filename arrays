#include<iostream>
using namespace std;
int main(){
    int n=0;
    cout<<"enter no of elements in array"<<endl;
    cin>>n;
    int arr[n];
    cout<<"enter the elements\n";
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int *ptr;
    ptr=arr;
    for(int i=0;i<n;i++){
        cout<<*ptr<<endl;
        ++ptr;
    }
    
    return 0;
}
